{
    "name": "angular2-samples",
    "version": "1.0.0",
    "scripts": {
        "start:prod": "npm run tsc && SET NODE_ENV=production && SET PORT=80 && node server.js",
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:dev": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
        "start:container": "npm run tsc && npm run lite:container",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "lite": "lite-server",
        "lite:container": "lite-server -c docker-bs-config.json",
        "test:dev": "karma start",
        "test:circleci": "npm run tsc && karma start karma.conf.circleci.js"
    },
    "license": "ISC",
    "dependencies": {
        "@angular/common": "^4.3.5",
        "@angular/compiler": "^4.3.5",
        "@angular/core": "^4.3.5",
        "@angular/forms": "^4.3.5",
        "@angular/http": "^4.3.5",
        "@angular/platform-browser": "^4.3.5",
        "@angular/platform-browser-dynamic": "^4.3.5",
        "@angular/router": "^4.3.5",
        "@angular/upgrade": "^4.3.5",
        "bootstrap": "^3.3.7",
        "core-js": "^2.5.0",
        "d3": "^3.5.17",
        "d3-array": "^1.2.0",
        "d3-geo": "^1.10.0",
        "d3-geo-projection": "^1.2.3",
        "datamaps": "^0.5.8",
        "es6-promise": "^4.1.1",
        "es6-shim": "^0.35.3",
        "font-awesome": "^4.7.0",
        "i18n-iso-countries": "^1.15.1",
        "ngx-bootstrap": "^1.9.1",
        "pad": "^1.1.0",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.4.3",
        "systemjs": "^0.20.17",
        "systemjs-plugin-json": "^0.2.3",
        "topojson": "^2.2.0",
        "zone.js": "^0.8.16"
    },
    "devDependencies": {
        "@types/core-js": "^0.9.42",
        "@types/d3": "^4.10.0",
        "@types/d3-array": "^1.2.1",
        "@types/d3-geo": "^1.6.3",
        "@types/es6-shim": "^0.31.34",
        "@types/jasmine": "^2.5.53",
        "@types/node": "^7.0.41",
        "concurrently": "^3.5.0",
        "express": "^4.16.3",
        "fs": "^0.0.1-security",
        "if-env": "^1.0.0",
        "jasmine": "^2.7.0",
        "jasmine-core": "^2.7.0",
        "karma": "^2.0.4",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^1.1.0",
        "karma-mocha-reporter": "^2.2.3",
        "karma-phantomjs-launcher": "^1.0.4",
        "lite-server": "^2.4.0",
        "path": "^0.12.7",
        "tslib": "^1.7.1",
        "tslint": "^5.11.0",
        "typescript": "^2.4.2"
    },
